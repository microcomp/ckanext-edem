#: ckanext/edem/plugin.py:133
#, python-format
msgid "User %s not authorized to create packages"
msgstr "Používateľ %s nemá oprávnenie vytvárať datasety"

#: ckanext/edem/plugin.py:137 ckanext/edem/plugin.py:183
#, python-format
msgid "User %s not authorized to edit these groups"
msgstr "Používateľ %s nemá oprávnenie upravovať tieto skupiny"

#: ckanext/edem/plugin.py:144
#, python-format
msgid "User %s not authorized to add dataset to this organization"
msgstr "Používateľ %s nemá oprávnenie pridávať datasety k tejto organizácií"

#: ckanext/edem/plugin.py:177
#, python-format
msgid "User %s not authorized to edit package %s"
msgstr "Používateľ %s nemá oprávnenie upravovať dataset %s"

#: ckanext/edem/plugin.py:224
msgid "Group was not found."
msgstr "Skupiny sa nenašla."

#: ckanext/edem/plugin.py:299
msgid "Only authenticated users are allowed to create applications"
msgstr "Iba prihlásení používatelia môžu vytvárať aplikácie."

#: ckanext/edem/plugin.py:310 ckanext/edem/plugin.py:320
#: ckanext/edem/plugin.py:330 ckanext/edem/plugin.py:335
msgid ""
"Only application owner and application administrators are allowed to edit"
" applications"
msgstr "Iba tvorca aplikácie a administrátor aplikácii môžu upravovať aplikáciu."

#: ckanext/edem/plugin.py:342
msgid "Only data curator is authorized to manage storage usage."
msgstr "Iba dátový kurátor má povolenie spravovať riadenie úložísk."

#: ckanext/edem/plugin.py:349
msgid "Only moderator is authorized to manage comments and blogs."
msgstr "Iba moderátor má povolenie spravovať komentáre a blogy."

#: ckanext/edem/plugin.py:356
msgid "Only data curator is authorized to manage tags of datasets."
msgstr "Iba dátový kurátor má povolenie spravovať navrhnuté tagy datasetov."

